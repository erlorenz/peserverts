{
  "name": "pressexpressserver",
  "version": "1.0.0",
  "description": "press express node server app",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "dev": "ts-node-dev src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "deploy:staging": "npm run build && gcloud config set project press-express-staging && gcloud app deploy staging.yaml",
